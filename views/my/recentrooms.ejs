        function goAppStore(dialogCheck){

            var url = location.protocol + '//m.goodchoice.kr/main/adKeywordLanding/';
            var kcode = getCookie('k_code');

            if(kcode && kcode.indexOf("^") > -1) {
                if(kcode.indexOf("du_") === 0) {
