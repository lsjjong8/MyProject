    <script>
      //스토어 랜딩 분기
      function goAppStore(dialogCheck) {

        var url = location.protocol + '//m.goodchoice.kr/main/adKeywordLanding/';
        var kcode = getCookie('k_code');

        if (kcode && kcode.indexOf("^") > -1) {
          if (kcode.indexOf("du_") === 0) {
            // 'du_' 다음 유입
            dialogCheck ? url += 'oK0nFLMJQJ2l' : url += 'oK0nFLMJQJ2j';
          } else if (kcode.indexOf("ns_") === 0) {
            // 'ns_' 네이버 지식쇼핑 유입
            dialogCheck ? url += 'ogMnDvZlsM2j' : url += 'ogynogydDJ2j';
          } else {
            // 그 외
            url += 'ogMnDvZlsM2j';
          }
        } else {
          // 그 외
          dialogCheck ? url += 'ogMnDvZlsM2b' : url += 'ogMnDvZlsM2j';
        }
        location.href = url;
      }
      // 쿠키 생성
      function setCookie(cName, cValue, cDay) {
        var expire = new Date();
        expire.setDate(expire.getDate() + cDay);
        cookies = cName + '=' + escape(cValue) + '; path=/ ';
        if (typeof cDay != 'undefined') cookies += ';expires=' + expire.toGMTString() + ';';
        document.cookie = cookies;
      }

      // 쿠키 가져오기
      function getCookie(cName) {
        cName = cName + '=';
        var cookieData = document.cookie;
        var start = cookieData.indexOf(cName);
        var cValue = '';
        if (start != -1) {
          start += cName.length;
          var end = cookieData.indexOf(';', start);
          if (end == -1) end = cookieData.length;
          cValue = cookieData.substring(start, end);
        }
        return unescape(cValue);
      }
      if (getCookie('marketing_type') != '') {
        if (getCookie('marketing_type') == 'pay') {
          var _nasa = {};
          _nasa["cnv"] = wcs.cnv("1", getCookie('marketing_price')); // 전환유형, 전환가치 설정해야함. 설치매뉴얼 참고
        } else {
          var _nasa = {};
          _nasa["cnv"] = wcs.cnv("2", "1"); // 전환유형, 전환가치 설정해야함. 설치매뉴얼 참고
        }

        //쿠키 삭제
        setCookie('marketing_price', '', -1);
        setCookie('marketing_type', '', -1);
      }
    </script>
