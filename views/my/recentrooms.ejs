<script>
    var totalpage = '0';

    $(document).ready(function () {

        // hash load or first load
        var pageHash = getCookie('recentPage');
        if(pageHash > 1){
            for(i=1; i <= pageHash; i++){
                recentAjaxLoad(i); // 최근본 숙소 로드
            }
        }else if($('li a[data-href]').length == 0) recentAjaxLoad(1); // page1 load

        $('ul.watch_list').on('click', 'li a[data-href]', function(){
            //location.hash = $('input[name=page]').val()==2 ? '' : $('input[name=page]').val();
            //location.hash = $('input[name=page]').val();

            setCookie('recentPage', $('input[name=page]').val(), 1);
            location.href = $(this).data('href');
        });

        $(document).scroll(function() {
            var currentPage = $('input[name=page]').val();
            var maxHeight = $(document).height();
            var currentScroll = $(window).scrollTop() + $(window).height();

            if(totalpage==0) return false;

            if (maxHeight <= currentScroll + 520) {
                recentAjaxLoad(currentPage); // 최근본 숙소 로드
            }
        });
    });

